{
  "name": "cote",
  "version": "0.16.2",
  "keywords": [
    "microservices",
    "zero-configuration",
    "zero-conf",
    "mesh",
    "network",
    "auto discovery",
    "scalable",
    "fault-tolerant"
  ],
  "description": "A Node.js library for building zero-configuration microservices.",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 -D -d dist -s",
    "clean": "rm -rf dist",
    "coverage": "nyc -x=\"**/examples/*\" -x=\"**/*balanced*.js\" -x=\"**/test/*.js\" report",
    "lint": "eslint src test",
    "lint-fix": "eslint --fix src test",
    "prepublish": "npm run build",
    "test": "NODE_ENV=dev nyc -x=\"**/examples/*\" -x=\"**/*balanced*.js\" -x=\"**/test/*.js\" ava --timeout=60000"
  },
  "author": {
    "name": "Armagan Amcalar",
    "email": "armagan@amcalar.com"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "repository": "dashersw/cote",
  "engines": {
    "node": ">=v4.0.0"
  },
  "dependencies": {
    "@dashersw/axon": "2.0.5",
    "@dashersw/node-discover": "^0.8.2",
    "charm": "1.0.2",
    "colors": "1.1.2",
    "eventemitter2": "4.1.2",
    "lodash": "4.17.5",
    "portfinder": "1.0.13",
    "socket.io": "2.0.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "async": "^2.6.0",
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "commander": "2.12.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.12.0",
    "eslint-config-google": "^0.9.1",
    "humanize-number": "0.0.2",
    "log-suppress": "^1.0.3",
    "nyc": "^11.3.0",
    "randomstring": "^1.1.5",
    "request": "^2.83.0",
    "sinon": "^4.1.2",
    "socket.io-client": "2.0.4"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "google"
    ],
    "rules": {
      "require-jsdoc": "off",
      "new-cap": "off",
      "guard-for-in": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "max-len": [
        "error",
        {
          "code": 120
        }
      ]
    }
  }
}
